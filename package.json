{
  "name": "@mindtickle/mt-ui-components",
  "version": "0.0.13-0",
  "description": "Collection of common React UI components for Mindtickle",
  "repository": "git@github.com:MindTickle/mt-ui-components.git",
  "author": "Afroz",
  "main": "./src/index.js",
  "license": "MIT",
  "scripts": {
    "story": "start-storybook -p 6006 -s ./public",
    "test": "jest --verbose ",
    "test:update": "jest -u",
    "start": "npm run story",
    "prebuild": "node utils/prebuild.js",
    "build:prod": "babel src --out-dir lib --copy-files --ignore test,*.stories.js && npm run test && npm run lint",
    "babel": "babel src --out-dir lib --copy-files --ignore test,*.stories.js",
    "babel:watch": "BABEL_ENV=dev babel src --out-dir lib --copy-files --ignore test,*.stories.js --watch",
    "build": "npm run lint && npm run prettier",
    "postbuild": "node utils/postbuild.js",
    "deploy:sb": "npm run build-storybook && gh-pages -m 'update storybook' -d ./build",
    "deploy": "sh ./deploy.sh",
    "build-storybook": "build-storybook -o ./build",
    "precommit": "npm run build && lint-staged",
    "prettier": "prettier --single-quote --write src/**/*.{js,scss}",
    "lint": "./node_modules/.bin/eslint src/**/*.js ",
    "lint:watch": "./node_modules/.bin/esw src/**/*.js -w"
  },
  "files": [
    "lib",
    "src",
    "README.md"
  ],
  "devDependencies": {
    "@storybook/addon-actions": "^3.4.8",
    "@storybook/addon-info": "^3.4.8",
    "@storybook/addon-knobs": "^3.4.8",
    "@storybook/addon-links": "^3.4.8",
    "@storybook/addon-options": "^3.4.8",
    "@storybook/addons": "^3.4.8",
    "@storybook/channel-postmessage": "^3.4.8",
    "@storybook/channels": "^3.4.8",
    "@storybook/cli": "^3.4.8",
    "@storybook/react": "^3.4.8",
    "babel-cli": "^6.18.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.5",
    "babel-jest": "^22.4.4",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-transform-class-properties": "^6.19.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.5",
    "babel-plugin-transform-es2015-modules-umd": "^6.24.1",
    "babel-plugin-transform-export-default": "^7.0.0-alpha.20",
    "babel-plugin-transform-react-require": "^1.0.1",
    "babel-plugin-transform-remove-strict-mode": "0.0.2",
    "babel-plugin-transform-require-ignore": "^0.0.2",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "6.11.1",
    "babel-preset-stage-2": "^6.24.1",
    "babel-runtime": "^6.22.0",
    "concurrently": "^4.0.1",
    "cross-env": "^3.1.4",
    "css-loader": "0.23.1",
    "css-modules-require-hook": "^4.0.5",
    "debug": "^2.6.0",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "^5.0.1",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-babel": "^5.1.0",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-jsx-a11y": "^6.1.0",
    "eslint-plugin-react": "^7.10.0",
    "eslint-watch": "^4.0.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^0.10.1",
    "gh-pages": "^0.12.0",
    "husky": "^0.14.3",
    "is-plain-object": "^2.0.4",
    "jest": "^22.4.4",
    "jest-css-modules": "^1.1.0",
    "jsdom": "^9.9.1",
    "lint-staged": "^7.0.0",
    "node-sass": "^4.9.0",
    "pre-commit": "^1.2.2",
    "prettier": "^1.13.7",
    "react-addons-test-utils": "^15.6.2",
    "react-hot-loader": "3.0.0-beta.6",
    "rimraf": "^2.6.2",
    "sass-loader": "^4.1.1",
    "style-loader": "^0.13.1",
    "styled-components": "^3.3.3",
    "stylelint": "^9.10.1",
    "stylelint-config-recess-order": "^2.0.1",
    "stylelint-config-standard": "^18.2.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-processor-styled-components": "^1.5.2",
    "url-loader": "^0.5.8",
    "webpack": "^3.12.0",
    "wix-storybook-utils": "^1.0.85"
  },
  "pre-commit": "precommit",
  "lint-staged": {
    "src/{,**/}*.{js,jsx,json,css,scss}": [
      "prettier --single-quote  --write",
      "git add"
    ],
    "src/{,**/}*.{js,jsx}": [
      "eslint"
    ]
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(css|less|scss)$": "identity-obj-proxy"
    },
    "testRegex": "^.+\\.test\\.js?$",
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/dist/"
    ],
    "automock": false,
    "setupFiles": [
      "./setupJest.js"
    ],
    "transform": {
      "^.+\\.js?$": "babel-jest"
    }
  },
  "dependencies": {
    "antd": "3.9.0",
    "babel-plugin-styled-components": "^1.6.3",
    "classnames": "^2.2.6",
    "deepmerge": "^2.2.1",
    "dom-css": "^2.1.0",
    "identity-obj-proxy": "^3.0.0",
    "install": "^0.11.2",
    "moment": "^2.22.2",
    "nanoid": "^1.0.2",
    "node-sass-utils": "^1.1.2",
    "npm": "^6.1.0",
    "prop-types": "^15.6.2",
    "react": "16.3.0",
    "react-copy-to-clipboard": "^5.0.1",
    "react-custom-scrollbars": "^4.2.1",
    "react-dom": "16.3.0",
    "react-icons": "^2.2.3",
    "react-infinite-scroller": "^1.2.1",
    "react-quill": "^1.3.3",
    "react-select": "^2.0.0",
    "react-sortable-tree": "2.2.0",
    "react-dnd": "3.0.2",
    "react-tagsinput": "^3.19.0",
    "react-test-renderer": "^16.1.1",
    "react-tether": "^0.5.7",
    "react-truncate": "^2.4.0",
    "trunc-html": "^1.1.2"
  },
  "peerDependencies": {
    "react": "16.3.0",
    "react-dom": "16.3.0",
    "styled-components": "^3.3.0"
  }
}
